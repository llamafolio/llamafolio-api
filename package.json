{
  "name": "llamafolio-api",
  "version": "0.0.1",
  "description": "LlamaFolio API",
  "scripts": {
    "adapter": "esno scripts/run-adapter.ts",
    "build-adapters": "esno scripts/build-adapters.ts",
    "build": "npm run build-adapters",
    "dev": "sls offline",
    "deploy:dev": "pnpm build && sls deploy --stage dev",
    "deploy:prod": "pnpm build && sls deploy --stage prod",
    "create-adapter": "esno scripts/create-adapter.ts",
    "delete-adapter": "esno scripts/delete-adapter.ts",
    "invalidate-protocols": "esno scripts/invalidate-protocols.ts",
    "provider-bench": "esno scripts/provider-bench.ts",
    "adapter-balances": "esno scripts/run-adapter-balances.ts",
    "update-balances": "esno scripts/update-balances.ts",
    "migrate": "node-pg-migrate",
    "revalidate-contracts": "esno scripts/revalidate-contracts.ts",
    "revalidate-contracts-rec": "esno scripts/revalidate-contracts-rec.ts",
    "find-user": "esno scripts/find-user.ts",
    "insert-tokens": "esno scripts/insert-tokens.ts",
    "update-tokens": "esno scripts/update-tokens.ts",
    "update-yields": "esno scripts/update-yields.ts",
    "update-protocols": "esno scripts/update-protocols.ts",
    "lint": "eslint --fix . --ext js,cjs,mjs,ts,d.ts,json --cache",
    "format": "prettier --write --ignore-path='./.eslintignore' '**/*.{md,json,js,ts}' --ignore-unknown --cache",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "dev-test": "sls offline --httpPort 3034",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "tscheck": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "ts-node": "esno",
    "preinstall": "npx only-allow pnpm"
  },
  "keywords": [],
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@defillama/sdk": "^3.0.8",
    "@llamafolio/tokens": "github:llamafolio/llamafolio-tokens",
    "aws-sdk": "^2.1189.0",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.9",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "ioredis": "^5.2.4",
    "jsbi": "^3.2.5",
    "lodash": "^4.17.21",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.101",
    "@types/eslint": "^8.37.0",
    "@types/jest": "^28.1.6",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.16.1",
    "@types/pg": "^8.6.5",
    "@types/pg-format": "^1.0.2",
    "@types/prettier": "^2.7.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitest/coverage-istanbul": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "bun": "^0.5.9",
    "bun-types": "^0.5.8",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "esno": "^0.16.3",
    "millify": "^5.0.1",
    "node-pg-migrate": "^6.2.2",
    "prettier": "^2.8.1",
    "serverless": "^3.30.1",
    "serverless-esbuild": "^1.43.1",
    "serverless-offline": "^12.0.4",
    "serverless-prune-plugin": "^2.0.2",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "packageManager": ">=pnpm@8.0.0",
  "engines": {
    "pnpm": ">=8.0.0",
    "yarn": "------>PLEASE USE PNPM<------",
    "npm": "------>PLEASE USE PNPM<------"
  }
}
